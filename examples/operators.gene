;; Test arithmetic operators
(print "=== Arithmetic Operators ===")
(print (+ 10 5))    ; 15
(print (- 10 5))    ; 5  
(print (* 10 5))    ; 50
(print (/ 10 5))    ; 2

;; Test comparison operators  
(print "=== Comparison Operators ===")
(print (< 5 10))    ; true
(print (> 5 10))    ; false
(print (== 5 5))    ; true
(print (!= 5 10))   ; true

;; Test some variables
(print "=== Variables ===")
(var x 42)
(var y 3)
(print x)
(print y)
(print (+ x y))