(fn test_simple_match [x]
  (match x
    (1 "one")
    (2 "two")
    (_ "other")))

(print (test_simple_match 1))
(print (test_simple_match 2))
(print (test_simple_match 3))

(fn test_variable_match [x]
  (match x
    (1 "exactly one")
    (n n)))

(print (test_variable_match 1))
(print (test_variable_match 42))