;; Simple class definition test
(class Point
  :fields (
    (x :int :public)
    (y :int :public)
  )
  :methods (
    (init (x y) (do
      (= this.x x)
      (= this.y y)
    ))
    (distance_from_origin () (do
      (print "Calculating distance...")
      (+ (* this.x this.x) (* this.y this.y))
    ))
  )
)

;; Test basic functionality
(print "Class definition loaded")