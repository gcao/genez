#!/usr/bin/env gene run

# Test: Simple native function tests

# Basic math
(print "sin(0) = " (sin 0))
(print "cos(0) = " (cos 0))
(print "sqrt(4) = " (sqrt 4))

# Type conversions
(print "int(3.14) = " (int 3.14))
(print "float(5) = " (float 5))
(print "str(42) = " (str 42))

# Collections
(print "len([1 2 3]) = " (len [1 2 3]))
(print "len(\"hello\") = " (len "hello"))

# Concatenation
(print "[1 2] ++ [3 4] = " (++ [1 2] [3 4]))
(print "\"hi\" ++ \"!\" = " (++ "hi" "!"))