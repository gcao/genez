#!/usr/bin/env gene run

# Test: Property field syntax (.prop name)

(class Point
  (.prop x)
  (.prop y))

(class Rectangle
  (.prop width)
  (.prop height)
  (.prop color))

# Fields with different types
(class Configuration
  (.prop debug)
  (.prop port)
  (.prop host)
  (.prop timeout))

# Multiple properties
(class Counter
  (.prop count)
  (.prop step)
  (.prop max))

(print "Field syntax parsed successfully")