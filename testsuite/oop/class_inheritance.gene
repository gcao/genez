#!/usr/bin/env gene run

# Test: Class inheritance with < syntax

# Basic inheritance
(class Animal
  (.prop sound))

(class Dog < Animal
  (.prop breed))

(class Cat < Animal
  (.prop color))

# Multi-level inheritance
(class Vehicle
  (.prop wheels))

(class Car < Vehicle
  (.prop doors))

(class SportsCar < Car
  (.prop turbo))

(print "Class inheritance parsed successfully")