#!/usr/bin/env gene run

# Test: Property declarations with (.prop name)

(class Person
  (.prop first_name)
  (.prop last_name)
  (.prop age))

(class Book
  (.prop title)
  (.prop author)
  (.prop isbn)
  (.prop pages))

# Mixed field declarations
(class Product
  (.prop id)
  (.prop name)
  (.prop price)
  (.prop in_stock))

(print "Instance variable declarations parsed successfully")