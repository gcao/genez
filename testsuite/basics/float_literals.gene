#!/usr/bin/env gene run

# Test: Float literal parsing and operations

# Basic float literals
(print 3.14)
(print 0.5)
(print 42.0)

# Float arithmetic
(print (+ 1.5 2.5))
(print (- 10.0 3.7))
(print (* 2.5 4.0))
(print (/ 10.0 3.0))

# Float comparisons
(print (< 3.14 4.0))
(print (> 5.5 3.3))
(print (== 2.0 2.0))
(print (== 2.0 2.1))

# Mixed int/float operations
(print (+ 1 2.5))
(print (* 3 1.5))